<launch>

    <arg name="pacmod_can_hardware_id" default="27791"/>
    <arg name="pacmod_can_circuit_id" default="0"/>

    <group ns="pacmod">

        <node pkg="pacmod" type="pacmod_node" name="pacmod">
            <param name="can_hardware_id" value="$(arg pacmod_can_hardware_id)"/>
            <param name="can_circuit_id" value="$(arg pacmod_can_circuit_id)"/>
        </node>

        <node pkg="joy" type="joy_node" name="joy">
              <param name="coalesce_interval" type="double" value="0.02"/>
        </node>

        <node pkg="pacmod_game_control" type="pacmod_game_control_node" name="pacmod_game_control"/>
	</group>
</launch>
